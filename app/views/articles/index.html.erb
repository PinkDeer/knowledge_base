<% @articles.each do |article| %>
  <h2><%= link_to article.title, article %></h2>
  <p>	Published at
	<%= article.created_at.strftime('%b %d, %Y') %>
  by
  <%= article.user.name %></p>
  <p><%= article.comments.count %> Comments</p>
  <p><%= article.get_likes.size %> Likes <%= article.get_dislikes.size %> Dilikes</p>
  <hr>
<% end %>
